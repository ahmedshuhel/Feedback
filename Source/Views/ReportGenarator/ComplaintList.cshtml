@model IList<ComplaintBox.Web.Models.Complaint>
<h2>Complaint's</h2>
<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Phone</th>
            <th>Address</th>
            <th>@ViewBag.Subjecttitle</th>
            <th>Complain</th>
            <th>Status</th>
           
        </tr>
    </thead>
    <tbody>
        
        @foreach(var complain in Model)
        {
            <tr>
                <td>@complain.Complainer</td>
                <td>@complain.PhoneNumber</td> 
                <td>@complain.Address</td>
                <td>@complain.Subject.Title</td>
                <td>@complain.Description</td>
                <td>@complain.Status</td>
                <td>@Html.ActionLink("Show Details", "ShowDetail", new {id = complain.Id})</td>   
            </tr>
        }            
    </tbody>
</table>
